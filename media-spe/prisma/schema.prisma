generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  name     String?
  password String
  role     String
}

model Articles {
  id        Int     @id @default(autoincrement())
  title     String
  content   String
  published Boolean @default(false)
  file      String
}
